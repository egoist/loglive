{
  "name": "loglive",
  "version": "0.2.2",
  "description": "Render changelog.md on the fly.",
  "repository": {
    "url": "egoist/loglive",
    "type": "git"
  },
  "main": "dist/cjs/loglive.js",
  "style": "dist/loglive.css",
  "unpkg": "dist/loglive.js",
  "cdn": "dist/loglive.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "changelog",
    "vue",
    "markdown"
  ],
  "scripts": {
    "test": "npm run lint && echo 'no tests!'",
    "lint": "xo",
    "example": "poi --config config/example.js",
    "build": "npm run build:cjs && npm run build:umd && rm -rf dist/cjs/*.css*",
    "build:cjs": "poi build --config config/shared.js --no-minimize",
    "build:umd": "poi build --config config/shared.js --dist dist --module-name LogLive --format umd",
    "prepublish": "npm run build",
    "toc": "markdown-toc -i README.md"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^1.28.5",
    "github-markdown-css": "^2.8.0",
    "marked3": "^0.5.1",
    "nano-assign": "^1.0.0",
    "poi": "^9.3.9",
    "promise-polyfill": "^6.0.2",
    "unfetch": "^3.0.0",
    "vue": "^2.4.4"
  },
  "devDependencies": {
    "bili": "^0.18.2",
    "eslint-config-rem": "^3.2.0",
    "eslint-plugin-vue": "beta",
    "markdown-toc": "^1.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": [
      "rem",
      "plugin:vue/recommended"
    ],
    "envs": [
      "browser"
    ],
    "rules": {
      "unicorn/no-abusive-eslint-disable": 0,
      "unicorn/filename-case": 0
    },
    "ignores": [
      "example/**"
    ],
    "extensions": [
      "vue"
    ]
  },
  "babel": {
    "presets": [
      [
        "vue-app",
        {
          "useBuiltIns": true
        }
      ]
    ]
  }
}
